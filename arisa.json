{
  "arisa": {
    "issues": {
      "projects": [
        "MC",
        "MCTEST",
        "MCPE",
        "MCAPI",
        "MCL",
        "MCD",
        "MCE",
        "BDS",
        "REALMS",
        "WEB"
      ],
      "url": "https://bugs.mojang.com/",
      "checkInterval": 10
    },
    "customFields": {
      "linked": "customfield_11100",
      "chkField": "customfield_10701",
      "confirmationField": "customfield_10500",
      "mojangPriorityField": "customfield_12200",
      "triagedTimeField": "customfield_12201"
    },
    "privateSecurityLevel": {
      "default": "10318",
      "special": {
        "MCL": "10502",
        "MCAPI": "10313"
      }
    },
    "helperMessages": {
      "updateInterval": 3600
    },
    "modules": {
      "attachment": {
        "resolutions": [
          "Awaiting Response",
          "Cannot Reproduce",
          "Done",
          "Duplicate",
          "Fixed",
          "Incomplete",
          "Invalid",
          "Unresolved",
          "Won't Fix",
          "Works As Intended"
        ],
        "extensionBlacklist": [
          "jar",
          "exe",
          "com",
          "bat",
          "msi",
          "run",
          "lnk",
          "dmg",
          "url"
        ]
      },
      "piracy": {
        "excludedStatuses": ["Postponed"],
        "message": "pirated-minecraft",
        "piracySignatures": [
          "Minecraft Launcher null",
          "Bootstrap 0",
          "Launcher: 1.0.10  (bootstrap 4)",
          "Launcher: 1.0.10  (bootstrap 5)",
          "Launcher 3.0.0",
          "Launcher: 3.1.0",
          "Launcher: 3.1.1",
          "Launcher: 3.1.4",
          "1.0.8",
          "uuid sessionId",
          "auth_access_token",
          "keicraft",
          "keinett",
          "nodus",
          "iridium",
          "mcdonalds",
          "uranium",
          "nova",
          "divinity",
          "gemini",
          "mineshafter",
          "Team-NeO",
          "DarkLBP",
          "Launcher X",
          "PHVL",
          "Pre-Launcher v6",
          "LauncherFenix",
          "TLauncher",
          "Titan"
        ]
      },
      "removeTriagedMeqs": {
        "meqsTags": [
          "MEQS_WAI",
          "MEQS_WONTFIX"
        ],
        "removalReason": "Ticket has been triaged."
      },
      "futureVersion": {
        "message": "provide-affected-versions"
      },
      "chk": {
        "excludedStatuses": ["Postponed"],
        "whitelist": [
          "MC",
          "MCL",
          "MCPE"
        ]
      },
      "confirmParent": {
        "confirmationStatusWhitelist": ["Unconfirmed", "Plausible"],
        "targetConfirmationStatus": ["Community Consensus"],
        "linkedThreshold": 3.0,
        "whitelist": [
          "MC",
          "MCL",
          "MCPE"
        ]
      },
      "reopenAwaiting": {
        "resolutions": [
          "Awaiting Response"
        ],
        "blacklistedRoles": [
          "staff",
          "global-moderators"
        ],
        "blacklistedRestrictions": [
          "helper",
          "staff",
          "global-moderators"
        ],
        "keepARTag": "MEQS_KEEP_AR"
      },
      "removeNonStaffMeqs": {
        "resolutions": [
          "Awaiting Response",
          "Cannot Reproduce",
          "Done",
          "Duplicate",
          "Fixed",
          "Incomplete",
          "Invalid",
          "Unresolved",
          "Won't Fix",
          "Works As Intended"
        ],
        "removalReason": "Comment was not properly staff-restricted."
      },
      "empty": {
        "excludedStatuses": ["Postponed"],
        "message": "incomplete"
      },
      "crash": {
        "excludedStatuses": ["Postponed"],
        "maxAttachmentAge": 30,
        "crashExtensions": [
          "txt",
          "log"
        ],
        "whitelist": [
          "MC",
          "MCL",
          "MCTEST"
        ],
        "duplicateMessage": "duplicate-tech",
        "moddedMessage": "modified-game",
        "duplicates": [
          {
            "type": "minecraft",
            "exceptionRegex":  "Pixel format not accelerated",
            "duplicates": "MC-297"
          },
          {
            "type": "minecraft",
            "exceptionRegex":  "No OpenGL context found in the current thread",
            "duplicates": "MC-297"
          },
          {
            "type": "minecraft",
            "exceptionRegex":  "Could not create context",
            "duplicates": "MC-297"
          },
          {
