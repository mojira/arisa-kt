        ]
      },
      "updateLinked": {
        "excludedStatuses": ["Postponed"],
        "updateInterval": 24,
        "whitelist": [
          "MC",
          "MCL",
          "MCPE"
        ]
      },
      "transferVersions": {
        "resolutions": [ "Duplicate" ]
      },
      "transferLinks": {
        "resolutions": [ "Duplicate" ]
      },
      "replaceText": {
        "resolutions": [
          "Awaiting Response",
          "Cannot Reproduce",
          "Done",
          "Duplicate",
          "Fixed",
          "Incomplete",
          "Invalid",
          "Unresolved",
          "Won't Fix",
          "Works As Intended"
        ]
      }
    }
  }
